 <a class=\"navbar-brand\" href=\"#\">Dashboard</a>\n            <div class=\"d-flex\">\n              <ul class=\"navbar-nav\">\n                <li class=\"nav-item\">\n                  <a @click=\"logoutAction()\" class=\"nav-link\" aria-current=\"page\" href=\"#\">Logout</a>\n                </li>\n              </ul>\n            </div>\n          </div>\n        </nav>\n        <h2 class=\"text-center mt-5\">Welcome, {{ user?.name }}!</h2>\n        <div>\n          <div>\n            <select v-model=\"column\">\n              <option :value=\"null\">No Column Filter</option>\n              <option v-for=\"col in cols\" :key=\"col\">{{ col }}</option>\n            </select>\n            <input type=\"text\" v-model=\"search\" placeholder=\"Search\">\n          </div>\n          <table>\n            <thead>\n              <tr>\n                <th v-for=\"col in cols\" :key=\"col\">{{ titles }}</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"row in rows\" :key=\"row.id\">\n                <td v-for=\"col in cols\" :key=\"col\">{{ results[col] }}</td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n\n      </div>\n    </div>\n  </layout-div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport LayoutDiv from \"../LayoutDiv.vue\";\n\nexport default {\n  name: \"DashboardPage\",\n  components: {\n    LayoutDiv\n  },\n  data() {\n    return {\n      user: {},\n      results: [],\n      titles: [],\n    };\n  },\n  created() {\n    this.getUser();\n    this.load_book_list();\n    //  if(localStorage.getItem('token') == \"\" || localStorage.getItem('token') == null){\n    //    this.$router.push('/')\n    //  }else {\n    //    this.getUser();\n    //  }\n  },\n  methods: {\n    getUser() {\n      axios\n        .get(\"/api/user\", {\n          headers: { Authorization: \"Bearer \" + localStorage.getItem(\"token\") },\n        })\n        .then((r) => {\n          this.user = r.data;\n          return r;\n        })\n        .catch((e) => {\n          return e;\n        });\n    },\n    logoutAction() {\n      localStorage.setItem(\"token\", \"\");\n      this.$router.push(\"/\");\n    },\n    load_book_list() {\n      axios\n        .get(\"http://127.0.0.1:5000/book_list\")\n        .then((r) => {\n          console.log(r.data);\n          this.results = r.data;\n          for (var i = 0; i < r.data.length; i++) {\n            this.titles.push(r.data[i].title);\n          }\n          console.log(this.titles);\n        })\n        .catch((e) => {\n          return e;\n        });\n    },\n  },\n};\n</script>\n"],"mappings":";AA6CA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,kBAAkB;AAExC,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,CAAC,CAAC;MACRC,OAAO,EAAE,EAAE;MACXC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,OAAO,CAAC,CAAC;IACd,IAAI,CAACC,cAAc,CAAC,CAAC;IACrB;IACA;IACA;IACA;IACA;EACF,CAAC;EACDC,OAAO,EAAE;IACPF,OAAOA,CAAA,EAAG;MACRT,KAAI,CACDY,GAAG,CAAC,WAAW,EAAE;QAChBC,OAAO,EAAE;UAAEC,aAAa,EAAE,SAAQ,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO;QAAE;MACtE,CAAC,EACAC,IAAI,CAAEC,CAAC,IAAK;QACX,IAAI,CAACb,IAAG,GAAIa,CAAC,CAACd,IAAI;QAClB,OAAOc,CAAC;MACV,CAAC,EACAC,KAAK,CAAEC,CAAC,IAAK;QACZ,OAAOA,CAAC;MACV,CAAC,CAAC;IACN,CAAC;IACDC,YAAYA,CAAA,EAAG;MACbN,YAAY,CAACO,OAAO,CAAC,OAAO,EAAE,EAAE,CAAC;MACjC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;IACxB,CAAC;IACDd,cAAcA,CAAA,EAAG;MACfV,KAAI,CACDY,GAAG,CAAC,iCAAiC,EACrCK,IAAI,CAAEC,CAAC,IAAK;QACXO,OAAO,CAACC,GAAG,CAACR,CAAC,CAACd,IAAI,CAAC;QACnB,IAAI,CAACE,OAAM,GAAIY,CAAC,CAACd,IAAI;QACrB,KAAK,IAAIuB,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIT,CAAC,CAACd,IAAI,CAACwB,MAAM,EAAED,CAAC,EAAE,EAAE;UACtC,IAAI,CAACpB,MAAM,CAACiB,IAAI,CAACN,CAAC,CAACd,IAAI,CAACuB,CAAC,CAAC,CAACE,KAAK,CAAC;QACnC;QACAJ,OAAO,CAACC,GAAG,CAAC,IAAI,CAACnB,MAAM,CAAC;MAC1B,CAAC,EACAY,KAAK,CAAEC,CAAC,IAAK;QACZ,OAAOA,CAAC;MACV,CAAC,CAAC;IACN;EACF;AACF,CAAC","ignoreList":[]},"